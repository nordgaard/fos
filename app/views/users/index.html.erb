<head>Look at all these Drinkers!!!</head>

<div class="row">
  <% @users.each do |user| %>
  <div class="col-md-3">
    <div class="thumbnail">
      <a href="/users/<%= user.id %>">
        <p><%= user.first_name %></p>
        <p><%= user.last_name %></p>
        <p><%= user.email %></p>
      </a>
    </div>
  </div>
  <% end %>
</div>
